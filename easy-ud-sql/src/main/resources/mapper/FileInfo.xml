<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bean.FileInfo">
    <select id="findById" resultType="com.sucsoft.ud.bean.FileInfo">
        select *
        from fileInfo
        where id = #{id}
    </select>
    <select id="findByMd5" resultType="com.sucsoft.ud.bean.FileInfo">
        select *
        from fileInfo
        where md5 = #{md5}
    </select>
    <select id="findAll" resultType="com.sucsoft.ud.bean.FileInfo">
        select *
        from fileInfo
        where 1=1
        order by date desc
    </select>
</mapper>